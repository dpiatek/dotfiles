[user]
	name = Dominik Piatek
	email = dominik.piatek@econsultancy.com
[core]
	excludesfile = /Users/piatek/.gitignore_global
[alias]
  s = status
  a = add
  q = commit
  c = checkout
  d = diff
  pushci = push ci HEAD
  pushup = push origin HEAD
  l = log --graph --pretty=format:'%Cred%h%Creset - %s (%Cgreen%cr, %C(yellow)%an%Creset)' --abbrev-commit --date=relative
  lm = log --graph --pretty=format:'%Cred%h%Creset - %s (%Cgreen%cr, %C(yellow)%an%Creset)' master.. --abbrev-commit --date=relative
  la = log --graph --abbrev-commit --date=relative --all --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s (%Cgreen%cr, %an%Creset)'
[color]
  branch = auto
  diff = auto
  status = auto
	sh = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan

